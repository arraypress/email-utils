{
  "name": "arraypress/email-utils",
  "description": "An immutable value object for working with email addresses, providing parsing, validation, transformation, and analysis.",
  "version": "1.0.0",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/email-utils",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "keywords": [
    "email",
    "validation",
    "parser",
    "spam-score",
    "disposable",
    "subaddress"
  ],
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\EmailUtils\\": "src/"
    },
    "files": [
      "src/Utilities/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ArrayPress\\EmailUtils\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/email-utils/issues",
    "source": "https://github.com/arraypress/email-utils"
  }
}